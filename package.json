{
  "name": "agent-starter-node",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{ts,js}\"",
    "lint:fix": "eslint --fix \"**/*.{ts,js}\"",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "test": "node --test --import tsx",
    "test:watch": "node --test --watch --import tsx",
    "dev": "tsx src/server.ts",
    "dev:agent": "tsx src/agent.ts dev",
    "download-files": "pnpm run build && node dist/agent.js download-files",
    "start": "node dist/server.js",
    "start:agent": "node dist/agent.js start"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.18.0",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  },
  "dependencies": {
    "@livekit/agents": "^1.0.0",
    "@livekit/agents-plugin-cartesia": "^1.0.0",
    "@livekit/agents-plugin-deepgram": "^1.0.0",
    "@livekit/agents-plugin-livekit": "^1.0.0",
    "@livekit/agents-plugin-openai": "^1.0.0",
    "@livekit/agents-plugin-silero": "^1.0.0",
    "@livekit/noise-cancellation-node": "^0.1.9",
    "livekit-client": "^2.15.7",
    "livekit-server-sdk": "^2.13.3",
    "openai": "^4.0.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "zod": "^3.25.76"
  }
}
